{
    "id": null,
    "uid": "aetherlink_business_kpis",
    "title": "AetherLink â€“ Business KPIs (Executive View)",
    "timezone": "browser",
    "schemaVersion": 39,
    "version": 1,
    "editable": true,
    "style": "dark",
    "refresh": "1m",
    "time": {
        "from": "now-24h",
        "to": "now"
    },
    "timepicker": {
        "refresh_intervals": [
            "30s",
            "1m",
            "5m",
            "15m"
        ]
    },
    "templating": {
        "list": []
    },
    "annotations": {
        "list": []
    },
    "panels": [
        {
            "type": "stat",
            "title": "Estimated 30-Day Cost",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 0
            },
            "datasource": {
                "type": "prometheus",
                "uid": "Prometheus"
            },
            "targets": [
                {
                    "refId": "A",
                    "expr": "aether:estimated_cost_30d_usd"
                }
            ],
            "options": {
                "reduceOptions": {
                    "calcs": [
                        "lastNotNull"
                    ],
                    "values": false
                },
                "orientation": "horizontal",
                "colorMode": "value",
                "graphMode": "area",
                "justifyMode": "center",
                "textMode": "value_and_name"
            },
            "fieldConfig": {
                "defaults": {
                    "unit": "currencyUSD",
                    "decimals": 2,
                    "mappings": [
                        {
                            "type": "special",
                            "options": {
                                "match": "null"
                            },
                            "display": "No data"
                        }
                    ],
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green"
                            },
                            {
                                "value": 50,
                                "color": "yellow"
                            },
                            {
                                "value": 200,
                                "color": "red"
                            }
                        ]
                    }
                },
                "overrides": []
            }
        },
        {
            "type": "gauge",
            "title": "System Health Score",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 0
            },
            "datasource": {
                "type": "prometheus",
                "uid": "Prometheus"
            },
            "targets": [
                {
                    "refId": "A",
                    "expr": "aether:health_score:15m"
                }
            ],
            "options": {
                "reduceOptions": {
                    "calcs": [
                        "lastNotNull"
                    ],
                    "values": false
                },
                "showThresholdLabels": true,
                "showThresholdMarkers": true
            },
            "fieldConfig": {
                "defaults": {
                    "unit": "percent",
                    "decimals": 0,
                    "min": 0,
                    "max": 100,
                    "mappings": [
                        {
                            "type": "special",
                            "options": {
                                "match": "null"
                            },
                            "display": "No data"
                        },
                        {
                            "type": "range",
                            "options": {
                                "from": 0,
                                "to": 60
                            },
                            "display": "Critical"
                        },
                        {
                            "type": "range",
                            "options": {
                                "from": 60,
                                "to": 80
                            },
                            "display": "Degraded"
                        },
                        {
                            "type": "range",
                            "options": {
                                "from": 80,
                                "to": 100
                            },
                            "display": "Healthy"
                        }
                    ],
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "red"
                            },
                            {
                                "value": 60,
                                "color": "yellow"
                            },
                            {
                                "value": 80,
                                "color": "green"
                            }
                        ]
                    }
                },
                "overrides": []
            }
        },
        {
            "type": "timeseries",
            "title": "Cost Trend (24h)",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 8
            },
            "datasource": {
                "type": "prometheus",
                "uid": "Prometheus"
            },
            "targets": [
                {
                    "refId": "A",
                    "expr": "aether:estimated_cost_30d_usd",
                    "legendFormat": "Estimated 30-day cost"
                }
            ],
            "options": {
                "legend": {
                    "displayMode": "list",
                    "placement": "bottom"
                },
                "tooltip": {
                    "mode": "single",
                    "sort": "none"
                }
            },
            "fieldConfig": {
                "defaults": {
                    "unit": "currencyUSD",
                    "decimals": 2,
                    "custom": {
                        "drawStyle": "line",
                        "lineInterpolation": "smooth",
                        "lineWidth": 2,
                        "fillOpacity": 10,
                        "showPoints": "never",
                        "spanNulls": true
                    },
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green"
                            },
                            {
                                "value": 50,
                                "color": "yellow"
                            },
                            {
                                "value": 200,
                                "color": "red"
                            }
                        ]
                    }
                },
                "overrides": []
            }
        },
        {
            "type": "timeseries",
            "title": "Health Score Trend (24h)",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 8
            },
            "datasource": {
                "type": "prometheus",
                "uid": "Prometheus"
            },
            "targets": [
                {
                    "refId": "A",
                    "expr": "aether:health_score:15m",
                    "legendFormat": "System health"
                }
            ],
            "options": {
                "legend": {
                    "displayMode": "list",
                    "placement": "bottom"
                },
                "tooltip": {
                    "mode": "single",
                    "sort": "none"
                }
            },
            "fieldConfig": {
                "defaults": {
                    "unit": "percent",
                    "decimals": 0,
                    "min": 0,
                    "max": 100,
                    "custom": {
                        "drawStyle": "line",
                        "lineInterpolation": "smooth",
                        "lineWidth": 2,
                        "fillOpacity": 20,
                        "showPoints": "never",
                        "spanNulls": true
                    },
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "red"
                            },
                            {
                                "value": 60,
                                "color": "yellow"
                            },
                            {
                                "value": 80,
                                "color": "green"
                            }
                        ]
                    }
                },
                "overrides": []
            }
        },
        {
            "type": "bargauge",
            "title": "Leads Claimed (Last 24h)",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 16
            },
            "datasource": {
                "type": "prometheus",
                "uid": "Prometheus"
            },
            "targets": [
                {
                    "refId": "A",
                    "expr": "sum by (actor) (increase(apexflow_leads_assigned_total[24h]))",
                    "legendFormat": "{{actor}}"
                }
            ],
            "options": {
                "orientation": "horizontal",
                "displayMode": "gradient",
                "showUnfilled": true,
                "text": {
                    "valueSize": 14
                }
            },
            "fieldConfig": {
                "defaults": {
                    "unit": "short",
                    "decimals": 0,
                    "min": 0,
                    "custom": {},
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green"
                            },
                            {
                                "value": 10,
                                "color": "yellow"
                            },
                            {
                                "value": 25,
                                "color": "orange"
                            }
                        ]
                    },
                    "color": {
                        "mode": "thresholds"
                    }
                },
                "overrides": []
            }
        },
        {
            "type": "timeseries",
            "title": "Lead Assignment Activity (24h)",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 16
            },
            "datasource": {
                "type": "prometheus",
                "uid": "Prometheus"
            },
            "targets": [
                {
                    "refId": "A",
                    "expr": "sum(rate(apexflow_leads_assigned_total[5m])) * 60",
                    "legendFormat": "Assignments/min"
                }
            ],
            "options": {
                "legend": {
                    "displayMode": "list",
                    "placement": "bottom"
                },
                "tooltip": {
                    "mode": "single",
                    "sort": "none"
                }
            },
            "fieldConfig": {
                "defaults": {
                    "unit": "short",
                    "decimals": 2,
                    "min": 0,
                    "custom": {
                        "drawStyle": "line",
                        "lineInterpolation": "smooth",
                        "lineWidth": 2,
                        "fillOpacity": 30,
                        "showPoints": "auto",
                        "spanNulls": true
                    },
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green"
                            }
                        ]
                    },
                    "color": {
                        "mode": "palette-classic"
                    }
                },
                "overrides": []
            }
        }
    ]
}